package Exercici9;

import Keyboard.*;

public class Exercici9 {
	public static void main(String args[]){
		int valor, resultat, residu;
		System.out.println("Especifica una quantitat");
		System.out.println("------------------------");
		valor = Keyboard.readInt();
		resultat = valor/20000;
		residu = valor%20000;
		System.out.println(resultat + " Bitllets de 20000 colons");
		resultat = residu/1000;
		System.out.println(resultat + " Bitllets de 10000 colons");
		residu = resultat/500;
		
	}
}
