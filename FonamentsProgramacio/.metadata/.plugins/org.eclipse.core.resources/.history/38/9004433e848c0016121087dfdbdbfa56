package Exercici9;

import Keyboard.*;

public class Exercici9 {
	public static void main(String args[]){
		int valor, resultat, residu;
		System.out.println("Especifica una quantitat");
		System.out.println("------------------------");
		valor = Keyboard.readInt();
		resultat = valor/2000;
		residu = valor%20000;
		System.out.println(resultat + "Bitllets de 2000 colons");
		resultat = residu/1000;
		System.out.println(resultat + "Bitllets de 1000 colons");
		
	}
}
