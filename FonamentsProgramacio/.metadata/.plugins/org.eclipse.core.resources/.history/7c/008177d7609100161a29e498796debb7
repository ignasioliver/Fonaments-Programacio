package Exercici9_prova;

import Keyboard.*;

public class Exercici9_prova2 { 
	public static void main(String args[]){
		int valor, resultat;
		System.out.println("Especifica una quantitat");
		System.out.println("------------------------");
		valor = Keyboard.readInt();
		resultat = valor / 50000;
		valor = valor % 50000;
		if (valor != 0){ System.out.println(resultat + " Bitllets de 50000 colons");}
		System.out.println(valor + " I passat a 20000:");
		resultat = valor / 20000;
		valor = valor % 20000;
		System.out.println(valor);
	}
}
