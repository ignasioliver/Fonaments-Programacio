package Exercici9_prova;

import Keyboard.*;

public class Exercici9prova {
	public static void main(String args[]){
		int valor, resultat, residu, resultat2, resultat3, resultat4, resultat5, resultat6;
		System.out.println("Especifica una quantitat");
		System.out.println("------------------------");
		valor = Keyboard.readInt();
		resultat = valor / 20000; //part entera
		valor = valor % 20000;//ara necessitem saber el residu (la part decimal)
		System.out.println(resultat);
		resultat = valor / 1000;
		valor = valor % 1000;
		System.out.println(resultat);
		resultat = valor / 500;
		valor = valor % 500;
		System.out.println(resultat);
		resultat = valor / 100;
		valor = valor % 100;
		System.out.println(resultat);
		resultat = valor / 25;
		valor = valor % 25;
		System.out.println(resultat);
		resultat = valor / 10;
		valor = valor % 10;
		System.out.println(resultat);
		resultat = valor / 1;
		valor = valor % 1;
		System.out.println(resultat);
		
		
		
	}
}
